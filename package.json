{
  "name": "better-datepicker",
  "version": "0.2.0",
  "description": "better-datepicker",
  "main": "dist/betterDatePicker.umd.js",
  "module": "dist/betterDatePicker.es5.js",
  "scripts": {
    "dev": "vite",
    "build:demo": "vite build",
    "serve": "vite preview",
    "build:css": "tsc  build/gulpfile.ts && gulp build --gulpfile build/gulpfile.js && rm -rf build/gulpfile.js",
    "prebp": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c build/rollup.config.ts && npm run build:locale && npm run build:css &&  rm -rf .rpt2_cache",
    "build:locale": "tsc build/locales.ts && node build/locales.js && rm -rf build/locales.js",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "lint": "eslint src --fix --ext .ts,.tsx"
  },
  "jest": {
    "preset": "./build/jest.config.ts"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/gulp-dart-sass": "^1.0.0",
    "@types/gulp-rename": "^2.0.0",
    "@types/jest": "^25.2.1",
    "@types/markdown-it": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "better-datepicker": "0.1.9",
    "dart-sass": "^1.25.0",
    "element-plus": "^1.0.2-beta.35",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "gulp-cssmin": "^0.2.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "highlight.js": "^9.16.2",
    "jest": "^25.0.0",
    "jest-config": "^25.0.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.2",
    "nprogress": "^0.2.0",
    "rollup": "^2.37.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.23.0",
    "ts-jest": "^25.4.0",
    "typescript": "^4.1.3",
    "vite": "^2.1.2",
    "vite-plugin-md": "^0.6.1",
    "vue": "^3.0.5",
    "vue-router": "4.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Js-Monkey/better-datepicker.git"
  },
  "author": "jumodada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Js-Monkey/better-datepicker/issues"
  },
  "homepage": "https://github.com/Js-Monkey/better-datepicker#readme"
}
