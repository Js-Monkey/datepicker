{
  "name": "bettter-datepicker",
  "version": "0.0.2",
  "description": "A date-picker",
  "main": "dist/betterDatePicker.umd.js",
  "module": "dist/betterDatePicker.es5.js",
  "scripts": {
    "dev": "vite",
    "build:demo": "vite build",
    "serve": "vite preview",
    "build:css": "gulp build --gulpfile build/gulpfile.js",
    "prebp": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c build/rollup.config.js && npm run build:css",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "lint": "eslint src --fix --ext .ts,.tsx"
  },
  "jest": {
    "preset": "./build/jest.config"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "animejs": "^3.1.0",
    "dart-sass": "^1.25.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "gulp-cssmin": "^0.2.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "highlight.js": "^9.16.2",
    "jest": "^25.0.0",
    "jest-config": "^25.0.0",
    "rollup": "^2.37.1",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.23.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^1.0.0",
    "text-loader": "^0.0.1",
    "transliteration": "^2.1.7",
    "ts-jest": "^25.4.0",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.16.11",
    "typescript": "^4.1.3",
    "vite": "^2.0.1",
    "vue": "^3.0.5",
    "vue-loader": "^15.7.1",
    "vue-router": "4.0.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Js-Monkey/better-datepicker.git"
  },
  "author": "jumodada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Js-Monkey/better-datepicker/issues"
  },
  "homepage": "https://github.com/Js-Monkey/better-datepicker#readme"
}
